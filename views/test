		<% @strs.each do |str| %>
			<% unless @workspace.keys.find_by(key: str).path == nil %>
				<% if @workspace.keys.find_by(key: str).data_type == "youtube" %>
					<iframe class="video" id="youtube_<%= str %>" width="100" height="100" src="https://www.youtube.com/embed/r1Ia4LhLQ6c?controls=0&rel=0&enablejsapi=1" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture"></iframe>
					<button id="play_<%= str %>">play</button>
					<button id="stop_<%= str %>">stop</button>
					<script>
						$(function(){
							$("#play_<%= str %>").on("click", function(){
								videoControl("playVideo");
							});

							$("#stop_<%= str %>").on("click", function(){
								videoControl("stopVideo");
							});

							function videoControl(action){
								var $playerWindow = $('#youtube_<%= str %>')[0].contentWindow;
								$playerWindow.postMessage('{"event":"command","func":"'+action+'","args":""}', '*');
							}
						});
					</script>
				<% end %>
			<% end %>
		<% end %>


							<div id="player_<%= str %>"></div>
					<button id="start_<%= str %>">play</button>
					<button id="stop_<%= str %>">stop</button>

					<script>

						var tag = document.createElement('script');

						tag.src = "https://www.youtube.com/iframe_api";
						var firstScriptTag = document.getElementsByTagName('script')[0];
						firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);

						var player_<%= str %>;
						function onYouTubeIframeAPIReady() {
							player_<%= str %> = new YT.Player('player_<%= str %>', {
								height: '100',
								width: '100',
								videoId: 'M7lc1UVf-VE',
							});
						}

						function playVideo() {
							player_<%= str %>.playVideo();
						}
						function stopVideo() {
							player_<%= str %>.stopVideo();
						}

						$("#start_<%= str %>").on("click", function(){
							playVideo();
						});

						$("#stop_<%= str %>").on("click", function(){
							stopVideo();
						});

					</script>